<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.googlecode.pongo.runtime.osgi">
      <class name="org.ossmeter.metricprovider.trans.dependencies.model.Container"/>
      <class name="org.ossmeter.metricprovider.trans.dependencies.model.Dependencies"/>
   </extension>
   <extension
         point="org.ossmeter.platform.metricprovider">
      <metricProvider
            provider="org.ossmeter.metricprovider.trans.dependencies.DependencyTransMetricProvider">
      </metricProvider>
   </extension>
      <extension
		point="org.ossmeter.platform.visualisation.metric">
		<visualisation
	    	json="vis/trans.dependency.json">
	   	</visualisation>
	</extension>
</plugin>
